{
  "name": "@ovee.js/toolkit-integrations",
  "author": "Owls Department",
  "license": "MIT",
  "version": "0.2.2",
  "description": "Ovee.js frontend toolkit developed by Owls Department",
  "bugs": {
    "url": "https://github.com/owlsdepartment/oveejs-toolkit/issues"
  },
  "homepage": "https://github.com/owlsdepartment/oveejs-toolkit#readme",
  "keywords": [
    "ui",
    "ovee.js",
    "toolkit",
    "components"
  ],
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./*": "./*",
    "./base-slider": {
      "types": "./base-slider.d.ts",
      "import": "./base-slider.mjs",
      "require": "./base-slider.cjs"
    },
    "./google-map": {
      "types": "./google-map.d.ts",
      "import": "./google-map.mjs",
      "require": "./google-map.cjs"
    },
    "./lazy-load": {
      "types": "./lazy-load.d.ts",
      "import": "./lazy-load.mjs",
      "require": "./lazy-load.cjs"
    },
    "./lottie-player": {
      "types": "./lottie-player.d.ts",
      "import": "./lottie-player.mjs",
      "require": "./lottie-player.cjs"
    },
    "./split-text": {
      "types": "./split-text.d.ts",
      "import": "./split-text.mjs",
      "require": "./split-text.cjs"
    },
    "./video-autoplay": {
      "types": "./video-autoplay.d.ts",
      "import": "./video-autoplay.mjs",
      "require": "./video-autoplay.cjs"
    }
  },
  "devDependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@ovee.js/toolkit": "workspace:^",
    "@types/google.maps": "^3.49.1",
    "@types/lodash": "^4.17.7",
    "@types/node": "^17.0.12",
    "gsap": "^3.12.5",
    "lodash": "^4.17.21",
    "lottie-web": "^5.12.2",
    "ovee.js": "^3.0.0-alpha.6",
    "swiper": "^11.1.12",
    "typescript": "^5.6.2",
    "vanilla-lazyload": "^19.1.3"
  },
  "peerDependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@ovee.js/toolkit": "workspace:^",
    "lodash": "^4.17.21",
    "lottie-web": "^5.12.2",
    "ovee.js": "^3.0.0-alpha.6",
    "swiper": "^11.1.12",
    "vanilla-lazyload": "^19.1.3"
  },
  "peerDependenciesMeta": {
    "@googlemaps/js-api-loader": {
      "optional": true
    },
    "lodash": {
      "optional": false
    },
    "lottie-web": {
      "optional": true
    },
    "ovee.js": {
      "optional": false
    },
    "swiper": {
      "optional": true
    },
    "vanilla-lazyload": {
      "optional": true
    }
  },
  "packageManager": "yarn@3.1.1"
}
